            @Html.DisplayFor(modelItem => item.PAIS1.NombrePais)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USUARIO.NombreUsuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VIAJERO.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
