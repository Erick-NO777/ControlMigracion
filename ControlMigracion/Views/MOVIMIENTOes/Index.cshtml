        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoSolicitud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PAIS.NombrePais)
        </td>
